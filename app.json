{
  "expo": {
    "name": "codediva",
    "slug": "codediva",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/png/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/png/splash-app.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/png/favicon.png"
    },
    "plugins": [
      "expo-font"
     
    ]
  }
}
